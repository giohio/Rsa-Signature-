{
  "name": "rsa-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start:api": "dotnet watch run --project ../../RsaSignApi/RsaSignApi",
    "start:web": "vite",
    "build": "tsc && vite build",
    "build:no-typecheck": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
